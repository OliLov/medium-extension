{
  "manifest_version": 3,
  "name": "Story Time",
  "version": "0.0.1",
  "description": "Track your story time.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/story-time-16.png",
    "48": "images/story-time-48.png",
    "128": "images/story-time-128.png"
  }
}
